<label><h1>Creating New Trip</h1></label>
  <%= form_for @trip do |f| %>
    <h3>Select <%= f.label :title %></h3>
    <p><%= f.text_field :title %></p>
    <h3>Select Destination</h3>
    <p><%= f.collection_select :destination_id, Destination.all, :id, :name %></p>
    <%= f.submit %>
  <% end %>
